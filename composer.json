{
    "name": "spatie/geocoder",
    "description": "Geocoding addresses to coordinates",
    "homepage": "https://github.com/spatie/geocoder",
    "keywords": [
        "geocode",
        "map",
        "coordinate",
        "location",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0|^13.0",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3|^12.5.12",
        "orchestra/testbench": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Geocoder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Geocoder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Geocoder\\GeocoderServiceProvider"
            ],
            "aliases": {
                "Geocoder": "Spatie\\Geocoder\\Facades\\Geocoder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
